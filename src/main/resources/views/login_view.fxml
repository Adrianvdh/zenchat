<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="regPane" prefHeight="418.0" prefWidth="382.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.zenchat.ui.login.LoginController">
   <children>
      <Button fx:id="loginButton" layoutX="68.0" layoutY="310.0" mnemonicParsing="false" onAction="#onLoginButtonClick" prefHeight="34.0" prefWidth="246.0" text="Login" />
      <TextField fx:id="userNameTextField" alignment="CENTER" layoutX="68.0" layoutY="200.0" prefHeight="34.0" prefWidth="246.0" promptText="User Name" />
      <PasswordField fx:id="passwordField" alignment="CENTER" layoutX="68.0" layoutY="254.0" prefHeight="34.0" prefWidth="246.0" promptText="Password" />
      <ImageView fitHeight="167.0" fitWidth="195.0" layoutX="101.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/zen-chat-icon.png" />
         </image>
      </ImageView>
      <Hyperlink fx:id="signUpLink" layoutX="165.0" layoutY="366.0" onAction="#onSignUpLinkClick" text="Sign Up" />
   </children>
</AnchorPane>
