<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="regPane" prefHeight="492.0" prefWidth="382.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.zenchat.ui.registration.RegistrationController">
   <children>
      <Button fx:id="registerButton" layoutX="68.0" layoutY="410.0" mnemonicParsing="false" onAction="#onRegisterButtonClick" prefHeight="34.0" prefWidth="119.0" text="Register" />
      <TextField fx:id="firstNameTextField" alignment="CENTER" layoutX="68.0" layoutY="195.0" prefHeight="34.0" prefWidth="246.0" promptText="First Name" />
      <Button fx:id="cancelButton" layoutX="195.0" layoutY="410.0" mnemonicParsing="false" onAction="#onCancelButtonClicked" prefHeight="34.0" prefWidth="119.0" text="Cancel" />
      <TextField fx:id="lastNameTextField" alignment="CENTER" layoutX="68.0" layoutY="248.0" prefHeight="34.0" prefWidth="246.0" promptText="Last Name" />
      <TextField fx:id="userNameTextField" alignment="CENTER" layoutX="68.0" layoutY="300.0" prefHeight="34.0" prefWidth="246.0" promptText="User Name" />
      <PasswordField fx:id="passwordField" alignment="CENTER" layoutX="68.0" layoutY="354.0" prefHeight="34.0" prefWidth="246.0" promptText="Password" />
      <ImageView fitHeight="167.0" fitWidth="195.0" layoutX="104.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/zen-chat-icon.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
